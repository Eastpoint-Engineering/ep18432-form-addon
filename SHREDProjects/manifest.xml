<?xml version="1.1" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" 
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
           xsi:type="MailApp">
  
  <Id>b2c3d4e5-f6a7-8901-bcde-f12345678901</Id>
  <Version>1.0.0.1</Version>
  <ProviderName>Eastpoint Engineering</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="SRED Projects"/>
  <Description DefaultValue="Opens a Microsoft Form to submit SR&amp;ED project requests for internal projects only."/>
  <IconUrl DefaultValue="https://eastpoint-engineering.github.io/ep18432-form-addon/SHREDProjects/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://eastpoint-engineering.github.io/ep18432-form-addon/SHREDProjects/icon-80.png"/>
  <SupportUrl DefaultValue="https://github.com/Eastpoint-Engineering/ep18432-form-addon/tree/main/SHREDProjects"/>
  
  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>
  
  <Requirements>
    <Sets>
      <Set Name="Mailbox" MinVersion="1.1"/>
    </Sets>
  </Requirements>
  
  <FormSettings>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://eastpoint-engineering.github.io/ep18432-form-addon/SHREDProjects/index.html"/>
        <RequestedHeight>350</RequestedHeight>
      </DesktopSettings>
    </Form>
  </FormSettings>
  
  <Permissions>ReadWriteMailbox</Permissions>
  
  <Rule xsi:type="RuleCollection" Mode="Or">
    <Rule xsi:type="ItemIs" ItemType="Message" FormType="Read"/>
  </Rule>
  
</OfficeApp>
